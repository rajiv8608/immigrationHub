/*Login page css*/

.login-bg {
    background: url("../images/Login_bg.jpg") no-repeat;
    background-size: cover;
    width: 100%;
    height: 100vh;

    
}
.login_side {
    background-color: $Secindary-color;
    color: $White;
    height: 100vh;
    width: 38%;
    margin: 0px;
    position: absolute;
    top: 0;
    left: 0px;
    bottom: 0;
}
.login-form {
    display: block;
    height: auto;
    overflow: hidden;
    margin-top: 10vh;
    margin-left: 100px;
}
a.more {
    text-decoration:underline !important;
    color:#1e6bb8 !important;
}
a.more:hover {
    text-decoration:none !important;
}
label {
    display: block;
}
.redstar {
    font-size: 12px;
    font-weight: bold;
    color:red;
}
.login_side {
    h2 {
        margin-bottom: 25px;
        font-size: 36px;
        font-weight: 200;
    }
    label {
        font-size: 14px;
        font-weight: 400;
    }
    input {
        width: 250px;
        height: 30px;
        margin-bottom: 15px;
        color: #999999;
        font-size: 14px;
        padding-left: 10px;
    }
    button {
        @include border-radius(0);
        width: 100px;
        background-color: #00b0e4;
        margin-bottom: 10px;
        font-size: 14px;
        font-weight: 700;
    }
    a {
        color: #fff;
        width: 100px;
        font-size: 12px;
        font-weight: 400;
        text-decoration: none;
    }
}
.login_text {
        margin-top: 30%;
    float: left;
    margin-left: 25%;
    p {
        width: 300px;
        font-size: 12px;
        font-weight: 400;
    }
    h4 {
        font-size: 18px;
        font-weight: 400;
    }
}
.loginbtn {
        background:#fff;
        color:#4d4d4d;
        padding:5px 15px;
        border-radius:5px;
        z-index:9999;
        cursor:pointer;
            float: right;
    margin: 20px 0 0 0;
    }
.login_maintext {
    margin-top: 61vh;
    background: rgba(255, 255, 255, 0.5);
    padding: 15px 35px;
    width: 650px;
    h4 {
        font-size: 32px;
        font-weight: 400;
        color: #333333;
    }
    p {
        width: 570px;
        font-size: 18px;
        font-weight: 400;
        color: #333333;
        text-align: justify;
    }
}
