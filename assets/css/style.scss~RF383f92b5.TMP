}
section {
    .left-opened, .rightwithmenu {
        position: absolute;
        top: 135px;
        bottom: 0px;
        height: calc(100vh - 135px);
    }
    .left-opened {
        left: 0px;
        width: 200px;
        background-color: #2f323a !important;
    }
    .left-closed {
        left: -200px;
        width: 200px;
        position: absolute;
        top: 135px;
    }
    .rightwithmenu {
        left: 200px;
        right: 0;
    }
    .rightwithoutmenu {
        left: 0px;
        right: 0;
        position: absolute;
        top: 135px;
    }
}
.navbar-nav {
    width: 100%;
    margin: 0px;
}
.navbar {
    @include border-radius(0);
}
.navbar-fixed-left {
    width:calc(100% - 15px);
   @include border-radius(0);
    height:calc(100% - 15px);
    border: none;
    overflow: auto;
    background-color:#2f323a;
    position: absolute;
    padding: 0px;
    background: #fff;
    border-radius: 4px;
    margin:0px 0px 0 15px;
    .navbar-nav {
        > li {
            float: none;
            display: block;
            position: relative;
            padding: 0;
            width: 100%;
            height: auto;
            margin-bottom:1px;
        }
        > li > .dropdown-menu {
            position: static !important;
            margin-top: 0px;
            margin-left: 0;
            border: 0px;
            width: 100%;
            @include border-radius(0);
            background-color: transparent;
            -webkit-transition: transform 0.2s linear;
            -moz-transition: transform 0.2s linear;
            transition: transform 0.2s linear;
        }
        > li > .dropdown-menu > li > a {
            display: block;
            text-shadow: none;
            font-size: 12px;
            text-decoration: none;
            color: #2f323a !important;
            padding-left: 45px;
            height: auto;
            line-height: 29px;
        }
        li .dropdown-menu li a:hover,
        li .dropdown-menu li a:focus,
        li .dropdown-menu li a.active {
            background-color: #5093e1 !important;
            color:#fff;
        }
        li.active a,
        li a.active,
        li.active a:hover > span,
        li.active a:focus > span {
            color: #5093e1;

        }
    }
}
.navbar-inverse .navbar-nav > .open > a {
    background-color: transparent;
    &:focus, &:hover {
        background-color: transparent;
    }
}
.navbar {} .left-opened .navbar-inverse,
.left-closed .navbar-inverse {
    .navbar-nav>li>a {
        display: block;
        height: 42px !important;
        padding: 0 0 0 13px !important;
        text-shadow: none;
        font-size: 14px !important;
        text-decoration: none;
        color: #2f323a;
        line-height: 42px !important;
        white-space: nowrap;
        overflow: hidden;
        -webkit-transition: transform 0.2s linear;
        -moz-transition: transform 0.2s linear;
        transition: transform 0.2s linear;
        span[class*="ico-"] {
            width: 12px;
            height: 12px;
            padding: 0 10px 0 3px;
            margin: 0 10px 0 0;
            display:none;
        }
        span[class*="ico-arrow"] {
            display:inline;
            }
        /*a.dropdown-toggle:after{
            opacity:0;
        }*/
    }
    .navbar-nav>li>a:hover,
    .navbar-nav>li>a:focus, .navbar-nav>li>a.active {
        color: #fff;
        background-color: #44474e !important;
    }
    li.dropdown span.ico-arrow {
        position: absolute;
        right: 0px;
        margin-top: 15px !important;
        background: url(../images/icons/down-arrow.png) no-repeat 0 0;
        -webkit-transition: transform 0.2s linear;
        -moz-transition: transform 0.2s linear;
        transition: transform 0.2s linear;
    }
    li.dropdown.open span.ico-arrow {
        -webkit-transform: rotate(180deg);
        -moz-transform: rotate(180deg);
        transform: rotate(180deg);
    }
}
